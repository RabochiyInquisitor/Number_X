{
  "expo": {
    "name": "Number_X",
    "slug": "numberx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.markus.Number_X"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b652f110-2e24-4f1d-8752-25bbca279ea2"
      }
    },
    "owner": "20markus08"
  }
}
